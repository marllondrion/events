{
  "$schema": "http://json.schemastore.org/template",
  "author": "@prince272",
  "classifications": [ "Web", "ASP.NET", "React.js", "Next.js" ],
  "identity": "NextSolution.Template",
  "name": "Next Solution (ASP.NET API & Next.js)",
  "description": "A starter solution template for creating a modern codebase for your upcoming application.",
  "shortName": "nextsoln",
  "sourceName": "NextSolution",
  "preferNameDirectory": true,
  "tags": {
    "type": "solution",
    "language": "C#"
  },
  "sources": [
    {
      "exclude": [

        "**/*.filelist",
        "**/*.lock.json",

        // User-specific files 
        "**/*.rsuser",
        "**/*.suo",
        "**/*.user",
        "**/*.userosscache",
        "**/*.sln.docstates",

        // Build directories
        "**/[Dd]ebug/**",
        "**/[Dd]ebugPublic/**",
        "**/[Rr]elease/**",
        "**/[Rr]eleases/**",
        "**/x64/**",
        "**/x86/**",
        "**/[Ww][Ii][Nn]32/**",
        "**/[Aa][Rr][Mm]/**",
        "**/[Aa][Rr][Mm]64/**",
        "**/bld/**",
        "**/[Bb]in/**",
        "**/[Oo]bj/**",
        "**/[Oo]ut/**",
        "**/[Ll]og/**",
        "**/[Ll]ogs/**",

        // Template directories
        ".template.config/**/*",

        // Visual Studio 2015/2017 cache/options directories
        "**/.vs/**",

        // Git directories
        "**/.git/**",
        "**/*.gitattributes",
        "**/*.gitignore",

        "**/*.nuspec"
      ]
    }
  ],
  "symbols": {
    "DefaultDatabaseId": {
      "type": "generated",
      "generator": "guid",
      "replaces": "{{DefaultDatabaseId}}",
      "parameters": {
        "format": "N"
      }
    }
  }
}